<%= form_for(@tour) do |f| %>
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>

    <%= f.text_field :destinations_attributes, list: "destination_autocomplete" %>
    <datalist id="destination_autocomplete">
      <% Destination.all.each do |d| %>
        <option value="<%= (d.city) %>">
      <% end %>
    </datalist><br>
  

    <%= f.label 'Price:' %><br>
    <%= f.number_field :price%><br>

    <%= f.label 'Duration:' %><br>
    <%= f.number_field :duration %><br>

    <%= f.label 'Tour Description:' %><br>
    <%= f.text_area :description %><br>

    <%= f.submit %> 
<% end %>